@model IEnumerable<ContractMonthlyClaimSys.Models.Claim>

<h2>Check Claim Status</h2>
<table>
    <thead>
        <tr>
            <th>Claim ID</th>
            <th>Lecturer Name</th>
            <th>Total Amount</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Id</td>
                <td>@claim.LecturerName</td>
                <td>@claim.TotalAmount</td>
                <td>@claim.Status</td>
            </tr>
        }
    </tbody>
</table>
